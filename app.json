{
  "expo": {
    "scheme": "geo-app",
    "name": "Geography App",
    "slug": "geo-app",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "android": {
      "package": "com.vasilevski.geoapp"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/Geographylogo.png",
    "splash": {
      "image": "./app/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}